    <Grid>
        <Grid.ColumnDefinitions >
            <ColumnDefinition Width="0.1*" />
            <ColumnDefinition Width="0.3*" />
            <ColumnDefinition Width="0.1*" />
            <ColumnDefinition />
            <ColumnDefinition Width="0.08*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions >
            <RowDefinition Height="0.9*" />
            <RowDefinition Height="auto" />
            <RowDefinition />
            <RowDefinition Height="36"  />
            <RowDefinition Height="0.9*" />
        </Grid.RowDefinitions>

        <ContentControl  Grid.Column="1" Grid.RowSpan="999" >
            <ContentControl.Style>
                <Style TargetType="ContentControl">
                    <Style.Triggers>
                        <DataTrigger Binding="{Binding Picture}" Value="{x:Static local:MsgBxPicture.Info}" >
                            <Setter Property="ContentTemplate" Value="{StaticResource vctImgInfo}" />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Picture}" Value="{x:Static local:MsgBxPicture.Question}" >
                            <Setter Property="ContentTemplate" Value="{StaticResource vctImgQuestion}" />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Picture}" Value="{x:Static local:MsgBxPicture.Critical}" >
                            <Setter Property="ContentTemplate" Value="{StaticResource vctImgCritical}" />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Picture}" Value="{x:Static local:MsgBxPicture.Alert}" >
                            <Setter Property="ContentTemplate" Value="{StaticResource vctImgAlert}" />
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </ContentControl.Style>
        </ContentControl>

        <TextBlock x:Name="lblMessage" Grid.Column="3" Grid.Row="1" TextWrapping="Wrap" TextAlignment="Center" Text="{Binding Text}" FontSize="18" />


        <ItemsControl Grid.Column="3" Grid.Row="3" x:Name="tmsCtrlButtonSet" ItemsSource="{Binding Path=ButtonSet}" >
            <ItemsControl.ItemsPanel>
                <ItemsPanelTemplate>
                    <UniformGrid Rows="1" />
                </ItemsPanelTemplate>
            </ItemsControl.ItemsPanel>
            <ItemsControl.ItemTemplate>
                <DataTemplate DataType="{x:Type system:String}">
                    <TextBlock x:Name="txbAction" Text="{Binding .}" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="15" MouseLeftButtonUp="TxbAction_MouseLeftButtonUp" >
                        <TextBlock.Style>
                            <Style TargetType="TextBlock">
                                <Setter Property="LayoutTransform" >
                                    <Setter.Value>
                                        <ScaleTransform ScaleX="1" ScaleY="1"/>
                                    </Setter.Value>
                                </Setter>
                                <Setter Property="Cursor" Value="Hand"/>

                                <Style.Triggers>
                                    <EventTrigger RoutedEvent="MouseEnter" >
                                        <BeginStoryboard Storyboard="{StaticResource stbZoom2x}" />
                                        <BeginStoryboard>
                                            <Storyboard  >
                                                <ColorAnimation Duration="0:0:0.5" Storyboard.TargetProperty="(Control.Foreground).Color" To="#8CD4FF" >
                                                    <ColorAnimation.EasingFunction>
                                                        <CubicEase />
                                                    </ColorAnimation.EasingFunction>
                                                </ColorAnimation>
                                            </Storyboard>
                                        </BeginStoryboard>

                                    </EventTrigger>
                                    <EventTrigger RoutedEvent="MouseLeave" >
                                        <EventTrigger.Actions>
                                            <BeginStoryboard Storyboard="{StaticResource stbZoom1x}" />
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <ColorAnimation Duration="0:0:0.5" Storyboard.TargetProperty="(Control.Foreground).Color" To="Black" >
                                                        <ColorAnimation.EasingFunction>
                                                            <CubicEase />
                                                        </ColorAnimation.EasingFunction>
                                                    </ColorAnimation>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger.Actions>
                                    </EventTrigger>
                                </Style.Triggers>
