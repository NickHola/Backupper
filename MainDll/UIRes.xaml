    <Style x:Key="stlBtnMenu" TargetType="mainCtrl:MenuButton">
        <Setter Property="FontFamily" Value="Segoe MDL2 Assets" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Padding" Value="0,7,0,7" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="Foreground" Value="#EAEAEA" />

        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Cursor" Value="Hand" />
                <Setter Property="Foreground" Value="#262626" />
            </Trigger>
            <Trigger Property="Selected" Value="True">
                <Setter Property="Foreground" Value="{Binding ForegroundSelectedColor, RelativeSource={RelativeSource Mode=Self}}" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="stlGpbChkMain" TargetType="mainCtrl:GroupBoxCheck">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="mainCtrl:GroupBoxCheck">
                    <Grid>
                        <!--Header="{TemplateBinding Text}"-->
                        <GroupBox x:Name="gpbMain" Content="{TemplateBinding Content}" IsEnabled="{TemplateBinding GpbIsEnabled}" Padding="{TemplateBinding Padding}" >
                            <GroupBox.Header>
                                <TextBlock Text="{TemplateBinding Text}" Margin="21,0,0,0" />
                            </GroupBox.Header>
                        </GroupBox>


                        <mainCtrl:ToggleButton x:Name="tbtIsEnabled" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="7,1,0,0"  
                                      IsChecked="{Binding GpbIsEnabled, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"/>

                        <!--<CheckBox x:Name="chkIsEnabled" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="7,2,0,0"  
                                      IsChecked="{Binding GpbIsEnabled, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"/>-->

                        <Rectangle x:Name="rctOverText" Width="{Binding Header.ActualWidth, ElementName=gpbMain}" HorizontalAlignment="Left" VerticalAlignment="Top" 
                                   Margin="27,5,0,0" MinWidth="8" Height="11" Fill="Transparent" Cursor="Hand" />
                        <!--<Rectangle.Style> <Style TargetType="Rectangle"> <Style.Triggers> <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="Cursor" Value="Hand" />
                                        </Trigger> </Style.Triggers> </Style> </Rectangle.Style>-->
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Padding" Value="0,5,0,0" />
    </Style>

    <Style x:Key="stlTxtErrorTemplate" TargetType="{x:Type mainCtrl:TextBoxM}">
        <Setter Property="Validation.ErrorTemplate">
            <Setter.Value>
                <ControlTemplate>
                    <Grid>
                        <Border x:Name="brdCella" BorderBrush="#FFF55A01" BorderThickness="1" IsHitTestVisible="False"/>
                        <AdornedElementPlaceholder x:Name="plhMain" />
                        <Popup AllowsTransparency="True" HorizontalAlignment="Right" HorizontalOffset="0" VerticalOffset="0" PopupAnimation="Fade" Placement="Right" 
                                   PlacementTarget="{Binding ElementName=brdCella}" IsOpen="{Binding ElementName=plhMain, Path=AdornedElement.IsFocused, Mode=OneWay}" >
                            <StackPanel Orientation="Horizontal">
                                <Polygon  VerticalAlignment="Center" Points="0,4 4,0 4,8" Stretch="Fill" StrokeThickness="0" 
                                              Fill="{StaticResource clrSfondoPopupErr}" />
                                <Border Background="{StaticResource clrSfondoPopupErr}" 
                                            CornerRadius="4" Padding="4">
                                    <TextBlock HorizontalAlignment="Center" FontWeight="Bold" Margin="2,0,0,0"
                                                   Foreground="{StaticResource clrScrittaPopupErr}"
                                                    Text="{Binding ElementName=plhMain, Path=AdornedElement.(Validation.Errors)[0].ErrorContent, Mode=OneWay}" />
                                    <!--Text="{Binding ElementName=plhMain, Path=AdornedElement.(Validation.Errors).las .ErrorContent, Mode=OneWay}" />-->
                                </Border>
                            </StackPanel>
                        </Popup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="stlTxtErrorTemplate2" TargetType="{x:Type mainCtrl:TextBoxM}">
        <Setter Property="Validation.ErrorTemplate">
            <Setter.Value>
                <ControlTemplate>
                    <Grid>
                        <Border x:Name="brdCella" BorderBrush="#FFF55A01" BorderThickness="1" IsHitTestVisible="False"/>
                        <AdornedElementPlaceholder x:Name="plhMain" />
                        <Popup AllowsTransparency="True" HorizontalAlignment="Right" HorizontalOffset="0" VerticalOffset="0" PopupAnimation="Fade" Placement="Right" 
                                   PlacementTarget="{Binding ElementName=brdCella}" IsOpen="{Binding ElementName=plhMain, Path=AdornedElement.IsFocused, Mode=OneWay}" >
                            <StackPanel Orientation="Horizontal">
                                <Polygon  VerticalAlignment="Center" Points="0,4 4,0 4,8" Stretch="Fill" StrokeThickness="0" 
                                              Fill="{StaticResource clrSfondoPopupErr}" />
                                <Border Background="{StaticResource clrSfondoPopupErr}" 
                                            CornerRadius="4" Padding="4">
                                    <TextBlock HorizontalAlignment="Center" FontWeight="Bold" Margin="2,0,0,0"
                                                   Foreground="{StaticResource clrScrittaPopupErr}"
                                                    Text="{Binding ElementName=plhMain, Path=AdornedElement.(Validation.Errors)[0].ErrorContent, Mode=OneWay}" />
                                    <!--Text="{Binding ElementName=plhMain, Path=AdornedElement.(Validation.Errors).las .ErrorContent, Mode=OneWay}" />-->
                                </Border>
                            </StackPanel>
                        </Popup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--#region DataGrid, celle e righe -->

    <Style x:Key="stlDtgClmHeader" TargetType="DataGridColumnHeader">
        <Setter Property="Margin" Value="0,-3,0,0" />
        <Setter Property="Height" Value="24" />
    </Style>

    <Style x:Key="stlDtgMain" TargetType="DataGrid">
        <Setter Property="HeadersVisibility" Value="Column" />
        <Setter Property="VerticalGridLinesBrush" Value="#E5F7FF" />
        <Setter Property="HorizontalGridLinesBrush" Value="#E5F7FF" />
        <Setter Property="ColumnHeaderHeight" Value="24" />
        <Style.Resources>
            <!-- Sfondo area vuota (dove non ci sono le celle) -->
            <SolidColorBrush x:Key="{x:Static SystemColors.ControlBrushKey}" Color="White" />
            <!-- Testo non evidenziato -->
            <SolidColorBrush x:Key="{x:Static SystemColors.ControlTextBrushKey}" Color="#1E1E1E" />
            <!-- Evidenziazione -->
            <!--<SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="#EFF9FF" />-->
            <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="Transparent" />
            <!-- Testo evidenziato -->
            <SolidColorBrush x:Key="{x:Static SystemColors.HighlightTextBrushKey}" Color="Black" />
            <Style TargetType="{x:Type DataGridCell}">
                <Style.Triggers>
                    <Trigger Property="IsFocused" Value="True">
                        <Setter Property="BorderBrush" Value="#AAD7FF"/>
                        <!--<Setter Property="FocusVisualStyle" Value="{x:Null}" />-->
                    </Trigger>
                </Style.Triggers>
            </Style>
        </Style.Resources>
    </Style>

    <Style x:Key="stlDtgCllTxtEditing" TargetType="{x:Type TextBox}">
        <Setter Property="Validation.ErrorTemplate">
            <Setter.Value>
                <ControlTemplate>
                    <Grid>
                        <Border x:Name="brdCella" BorderBrush="#FFF55A01" BorderThickness="1" IsHitTestVisible="False"/>
                        <AdornedElementPlaceholder x:Name="plhMain" />
                        <Popup AllowsTransparency="True" HorizontalAlignment="Right" HorizontalOffset="0" VerticalOffset="0" PopupAnimation="Fade" Placement="Right" 
                                   PlacementTarget="{Binding ElementName=brdCella}" IsOpen="{Binding ElementName=plhMain, Path=AdornedElement.IsFocused, Mode=OneWay}" >
                            <StackPanel Orientation="Horizontal">
                                <Polygon  VerticalAlignment="Center" Points="0,4 4,0 4,8" Stretch="Fill" StrokeThickness="0" 
                                              Fill="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type mainCtrl:DataGridM}}, Path=ColoreSfondoPopupErr}" />
                                <Border Background="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type mainCtrl:DataGridM}}, Path=ColoreSfondoPopupErr}" 
                                            CornerRadius="4" Padding="4">
                                    <TextBlock HorizontalAlignment="Center" FontWeight="Bold" Margin="2,0,0,0"
                                                   Foreground="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type mainCtrl:DataGridM}}, Path=ColoreScrittaPopupErr}"
                                                    Text="{Binding ElementName=plhMain, Path=AdornedElement.(Validation.Errors)[0].ErrorContent, Mode=OneWay}" />
                                    <!--Text="{Binding ElementName=plhMain, Path=AdornedElement.(Validation.Errors).las .ErrorContent, Mode=OneWay}" />-->
                                </Border>
                            </StackPanel>
                        </Popup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="stlDtgCllTxt" TargetType="{x:Type TextBlock}">
        <Style.Triggers>
            <Trigger Property="Validation.HasError" Value="true">
                <Setter Property="ToolTip" Value="{Binding RelativeSource={RelativeSource Self}, Path=(Validation.Errors)[0].ErrorContent}"/>
                <Setter Property="Background" Value="{Binding  RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type mainCtrl:DataGridM}}, Path=ColoreCellaInErr}"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="stlDtgRowValidErr" TargetType="{x:Type DataGridRow}" >
        <Style.Triggers>
            <!--ATTENZIONE: IsSelected deve stare prima di Validation.HasError perchè così quest'ultimo sovrascrive il valore Background di IsSelected, nello stile del DGV SystemColors.HighlightBrushKey deve essere transparent-->
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Background" Value="#EFF9FF"/>
            </Trigger>
            <Trigger Property="Validation.HasError" Value="True">
                <Setter Property="Background" Value="{Binding  RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type mainCtrl:DataGridM}}, Path=ColoreRigaInErr}"/>
                <!--<Setter Property="{x:Static SystemColors.HighlightBrushKey}" Value="{Binding  RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type mainCtrl:DataGridM}}, Path=ColoreRigaInErr}" />-->
                <Setter Property="ToolTip" Value="{Binding RelativeSource={RelativeSource Self}, Path=(Validation.Errors)[0].ErrorContent}"/>
                <!--<SolidColorBrush x:Key="{x:Static SystemColors.ControlBrushKey}" Color="Transparent" />
                <SolidColorBrush x:Key="{x:Static SystemColors.HighlightTextBrushKey}" Color="Black" />
                <SolidColorBrush x:Key="{x:Static SystemColors.ControlTextBrushKey}" Color="Black" />-->
            </Trigger>
        </Style.Triggers>
    </Style>

    <!--<ControlTemplate x:Key="ctrTplDtgRowValidErr">
        <Grid Margin="0,-2,0,-2" ToolTip="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type DataGridRow}}, Path=(Validation.Errors)[0].ErrorContent}">
            <Ellipse StrokeThickness="0" Fill="Red" Width="{TemplateBinding FontSize}" Height="{TemplateBinding FontSize}" />
            <TextBlock Text="!" FontSize="{TemplateBinding FontSize}" FontWeight="Bold" Foreground="White" HorizontalAlignment="Center" />
        </Grid>
    </ControlTemplate>-->

    <!--#endregion-->

    <Style x:Key="stlPsbMain" TargetType="PasswordBox">
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Margin" Value="5" />
    </Style>

    <!--#endregion-->

    <!--#region Data or Control Templates -->

    <DataTemplate x:Key="dttVoid" />

    <ControlTemplate x:Key="cttProgressRing" TargetType="Button">
        <Viewbox Stretch="Uniform">
            <MetroCtrl:ProgressRing IsLarge="True" EllipseDiameterScale="1.3" Foreground="{TemplateBinding Foreground}" />
        </Viewbox>
    </ControlTemplate>

    <ControlTemplate x:Key="cttStartSymbol" TargetType="Button">
        <Viewbox Stretch="Uniform">
            <Polygon Points="0,0 8,5, 0,10" Fill="{TemplateBinding Foreground}" />
        </Viewbox>
    </ControlTemplate>

    <ControlTemplate x:Key="cttStopSymbol" TargetType="Button">
        <Viewbox Stretch="Uniform">
            <Rectangle StrokeThickness="0" Margin="5,5,5,5" Width="100" Height="100" Fill="{TemplateBinding Foreground}" />
        </Viewbox>
    </ControlTemplate>

    <!--#endregion-->

    <!--#region Storyboard -->

    <Trigger x:Key="trgHiddenOnOpacity0" Property="Control.Opacity" Value="0">
        <Setter Property="Control.Visibility" Value="Hidden"/>
    </Trigger>
    
    <Storyboard x:Key="stbSoftOpenInHorizontal" x:Shared="False">
        <DoubleAnimation Storyboard.TargetProperty="(Control.LayoutTransform).(ScaleTransform.ScaleX)" From="0" To="1" Duration="0:0:0.4" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
        <DoubleAnimation Storyboard.TargetProperty="Opacity" From="0" To="1" BeginTime="0:0:0.2" Duration="0:0:0.8" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
    </Storyboard>
    <Storyboard x:Key="stbSoftCloseInHorizontal" x:Shared="False">
        <DoubleAnimation Storyboard.TargetProperty="Opacity" From="1" To="0" Duration="0:0:0.4" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
        <DoubleAnimation Storyboard.TargetProperty="(Control.LayoutTransform).(ScaleTransform.ScaleX)" From="1" To="0" BeginTime="0:0:0.2"  Duration="0:0:0.9" >
            <DoubleAnimation.EasingFunction >
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
    </Storyboard>

    <Storyboard x:Key="stbSoftOpenInVertical" x:Shared="False">
        <!--Il cambiamento della visibility serve per non renderizzare eventuali margini impostati sul sontrollo quando invisibile-->
        <!--<ObjectAnimationUsingKeyFrames BeginTime="0:0:0" Storyboard.TargetProperty="(UIElement.Visibility)">
            <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static Visibility.Collapsed}"/>
            <DiscreteObjectKeyFrame KeyTime="0:0:0.0000001" Value="{x:Static Visibility.Visible}"/>
        </ObjectAnimationUsingKeyFrames>-->
        <DoubleAnimation Storyboard.TargetProperty="(Control.LayoutTransform).(ScaleTransform.ScaleY)" From="0" To="1" Duration="0:0:0.4" >
            <!--<DoubleAnimation Storyboard.TargetProperty="(LayoutTransform).(ScaleTransform.ScaleY)" From="0" To="1" Duration="0:0:0.4" >-->
                <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
        <DoubleAnimation Storyboard.TargetProperty="Opacity" From="0" To="1" BeginTime="0:0:0.25" Duration="0:0:0.8" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
    </Storyboard>
    <Storyboard x:Key="stbSoftCloseInVertical" x:Shared="False">
        <DoubleAnimation Storyboard.TargetProperty="Opacity" To="0" Duration="0:0:0.4" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
        <DoubleAnimation Storyboard.TargetProperty="(Control.LayoutTransform).(ScaleTransform.ScaleY)" To="0" BeginTime="0:0:0.2"  Duration="0:0:0.9" >
            <!--<DoubleAnimation Storyboard.TargetProperty="(LayoutTransform).(ScaleTransform.ScaleY)" To="0" BeginTime="0:0:0.2"  Duration="0:0:0.9" >-->
                <DoubleAnimation.EasingFunction >
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
        <!--Il cambiamento della visibility serve per non renderizzare eventuali margini impostati sul sontrollo quando invisibile-->
        <!--<ObjectAnimationUsingKeyFrames BeginTime="0:0:0" Storyboard.TargetProperty="(UIElement.Visibility)">
            <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{x:Static Visibility.Visible}"/>
            <DiscreteObjectKeyFrame KeyTime="0:0:0.9" Value="{x:Static Visibility.Collapsed}"/>
        </ObjectAnimationUsingKeyFrames>-->
    </Storyboard>

    <Storyboard x:Key="stbZoom2x" x:Shared="False">
        <DoubleAnimation Duration="0:0:0.5" Storyboard.TargetProperty="(Control.LayoutTransform).(ScaleTransform.ScaleX)" To="2" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
        <DoubleAnimation Duration="0:0:0.5" Storyboard.TargetProperty="(Control.LayoutTransform).(ScaleTransform.ScaleY)" To="2" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
    </Storyboard>

    <Storyboard x:Key="stbZoom1_2x" x:Shared="False">
        <DoubleAnimation Duration="0:0:0.5" Storyboard.TargetProperty="(Control.LayoutTransform).(ScaleTransform.ScaleX)" To="1.2" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
        <DoubleAnimation Duration="0:0:0.5" Storyboard.TargetProperty="(Control.LayoutTransform).(ScaleTransform.ScaleY)" To="1.2" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
    </Storyboard>

    <Storyboard x:Key="stbZoom1x" x:Shared="False">
        <DoubleAnimation Duration="0:0:0.5" Storyboard.TargetProperty="(Control.LayoutTransform).(ScaleTransform.ScaleX)" To="1" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
        <DoubleAnimation Duration="0:0:0.5" Storyboard.TargetProperty="(Control.LayoutTransform).(ScaleTransform.ScaleY)" To="1" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
    </Storyboard>


    <Storyboard x:Key="stbSetOpacity100" x:Shared="False">
        <DoubleAnimation Storyboard.TargetProperty="Opacity" To="1" Duration="0:0:0.8"  >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
    </Storyboard>
    <Storyboard x:Key="stbSetOpacity80" x:Shared="False">
        <DoubleAnimation Storyboard.TargetProperty="Opacity" To="0.8" Duration="0:0:0.8" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
    </Storyboard>
    <Storyboard x:Key="stbSetOpacity0" x:Shared="False" >
        <DoubleAnimation Storyboard.TargetProperty="Opacity" To="0" Duration="0:0:0.8" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
    </Storyboard>

    <Storyboard x:Key="stbEnterRotate180" x:Shared="False">
        <DoubleAnimation Storyboard.TargetProperty="(Control.LayoutTransform).(RotateTransform.Angle)" From="0" To="180" Duration="0:0:0.8" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
        <!--<DoubleAnimation Storyboard.TargetProperty="Opacity" From="0" To="1" BeginTime="0:0:0.2" Duration="0:0:1.2" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>-->
    </Storyboard>
    <Storyboard x:Key="stbExitRotate180" x:Shared="False">
        <DoubleAnimation Storyboard.TargetProperty="(Control.LayoutTransform).(RotateTransform.Angle)" From="180" To="0" Duration="0:0:0.9" >
            <DoubleAnimation.EasingFunction>
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
        <!--<DoubleAnimation Storyboard.TargetProperty="(ScrollViewer.LayoutTransform).(ScaleTransform.ScaleX)" From="1" To="0" BeginTime="0:0:0.2"  Duration="0:0:0.9" >
            <DoubleAnimation.EasingFunction >
                <CubicEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>-->
    </Storyboard>

    <!--#endregion-->
    
