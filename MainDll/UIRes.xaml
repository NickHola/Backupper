<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:local="clr-namespace:Main"
    xmlns:mainCtrl="clr-namespace:Main.Controls" 
    xmlns:MetroCtrl="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"    >

    <KeySpline x:Key="KeySplineMain" ControlPoint1="0.09,1.05" ControlPoint2="0.78,1"/>
    <SolidColorBrush x:Key="clrSfondoPopupErr" Color="#E52914"/>
    <SolidColorBrush x:Key="clrScrittaPopupErr" Color="White"/>
    <SolidColorBrush x:Key="clrGenericBorder">#D8EFFF</SolidColorBrush>
    <!--<SolidColorBrush x:Key="clrGenericBorder">#C1E5FF</SolidColorBrush>-->
    
    <!--#region Styles -->

    <Style x:Key="stlTxtMain" TargetType="TextBox">
        <Setter Property="VerticalAlignment" Value="Center" />
    </Style>

    <Style x:Key="stlTxbMain" TargetType="mainCtrl:TextBlockM">
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="TextWrapping" Value="Wrap" />
    </Style>
    
